<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="299.0" prefWidth="362.0" xmlns="http://javafx.com/javafx/15.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PresentationLayer.RegisterPageController">
    <children>
        <ComboBox fx:id="selectAccountTypeCombo" layoutX="177.0" layoutY="32.0" onAction="#buttonAction"
                  prefHeight="25.0" prefWidth="149.0" promptText="Select Account Type"/>
        <Label fx:id="accountTypeLabel" layoutX="35.0" layoutY="36.0" prefHeight="17.0" prefWidth="87.0"
               text="Account Type"/>
        <Label fx:id="usernameLabel" layoutX="35.0" layoutY="87.0" text="Username"/>
        <Label fx:id="passwordLabel" layoutX="35.0" layoutY="121.0" text="Password"/>
        <Label fx:id="confirmPasswordLabel" layoutX="35.0" layoutY="156.0" text="Confirm Password"/>
        <TextField fx:id="usernameTextField" layoutX="176.0" layoutY="83.0" onAction="#buttonAction"
                   promptText="Insert Username"/>
        <PasswordField fx:id="passwordTextField" layoutX="176.0" layoutY="117.0" onAction="#buttonAction"
                       promptText="Insert Password"/>
        <PasswordField fx:id="confirmPasswordTextField" layoutX="176.0" layoutY="152.0" onAction="#buttonAction"
                       promptText="Confirm Password"/>
        <Button fx:id="registerButton" layoutX="177.0" layoutY="210.0" mnemonicParsing="false" onAction="#buttonAction"
                prefHeight="25.0" prefWidth="149.0" text="Register"/>
    </children>
</AnchorPane>
