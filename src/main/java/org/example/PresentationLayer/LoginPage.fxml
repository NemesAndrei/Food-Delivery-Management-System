<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="251.0" prefWidth="394.0" xmlns="http://javafx.com/javafx/15.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PresentationLayer.LoginPageController">
    <children>
        <Label fx:id="usernameLabel" layoutX="56.0" layoutY="47.0" prefHeight="36.0" prefWidth="125.0" text="Username"
               textAlignment="CENTER"/>
        <Label fx:id="passwordLabel" layoutX="56.0" layoutY="83.0" prefHeight="36.0" prefWidth="125.0" text="Password"/>
        <PasswordField fx:id="passwordTextField" layoutX="136.0" layoutY="89.0" onAction="#buttonAction"
                       prefHeight="25.0" prefWidth="200.0" promptText="Insert Password"/>
        <TextField fx:id="usernameTextField" layoutX="136.0" layoutY="53.0" onAction="#buttonAction" prefHeight="25.0"
                   prefWidth="200.0" promptText="Insert Username"/>
        <Button fx:id="loginButton" layoutX="56.0" layoutY="126.0" mnemonicParsing="false" onAction="#buttonAction"
                prefHeight="25.0" prefWidth="281.0" text="Login"/>
        <Button fx:id="registerButton" layoutX="56.0" layoutY="163.0" mnemonicParsing="false" onAction="#buttonAction"
                prefHeight="25.0" prefWidth="281.0" text="Register"/>
    </children>
</AnchorPane>
