                   prefWidth="93.0" promptText="Times Ordered"/>
        <TextField fx:id="reportThreeTextOne" layoutX="16.0" layoutY="173.0" onAction="#buttonAction" prefHeight="25.0"
                   prefWidth="93.0" promptText="Times Ordered"/>
        <TextField fx:id="reportThreeTextTwo" layoutX="16.0" layoutY="207.0" onAction="#buttonAction" prefHeight="25.0"
                   prefWidth="93.0" promptText="Total Value"/>
        <TextField fx:id="reportFourTextOne" layoutX="124.0" layoutY="173.0" onAction="#buttonAction" prefHeight="25.0"
                   prefWidth="93.0" promptText="Day"/>
        <TextField fx:id="reportFourTextTwo" layoutX="124.0" layoutY="207.0" onAction="#buttonAction" prefHeight="25.0"
                   prefWidth="93.0" promptText="Month"/>
